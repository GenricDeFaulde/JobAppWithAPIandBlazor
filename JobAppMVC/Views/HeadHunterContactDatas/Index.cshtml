@model IEnumerable<JobApp.Models.HeadHunterModel.HeadHunterContactData>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CompanyId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JobExchangeId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AddressNation)
